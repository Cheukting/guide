{
  "version": "1.0",
    "retry-policy": {
        "num-retries": 3,
        "reuse-merge": true
    },
    "builds": [
    {
      "job": {
        "name": "Build Guide"
      },
      "changed-files": [
        "/site/**",
        "/_includes/**",
        "/src/**",
        "/public/**",
        "/package.json",
        "/package-lock.json"
      ]
    }
  ]
}
