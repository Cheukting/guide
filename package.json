{
  "name": "g3-tsx",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node --require esbuild-register ./node_modules/@11ty/eleventy/cmd.js --config=./eleventy.config.js --serve --incremental",
    "build": "node --require esbuild-register ./node_modules/@11ty/eleventy/cmd.js && npm run demos",
    "demos": "copyfiles -e './_site/dotnet' -f './site/dotnet/**/demos/*' -f false --verbose && copyfiles -e './_site/goland' -f './site/goland/**/demos/*' -f false --verbose && copyfiles -e './_site/intellij' -f './site/intellij/**/demos/*' -f false --verbose && copyfiles -e './_site/pycharm' -f './site/pycharm/**/demos/*' -f false --verbose && copyfiles -e './_site/webstorm' -f './site/webstorm/**/demos/*' -f false --verbose",
    "clean": "rimraf _site .11ty-vite",
    "test": "vitest run",
    "broken-link-checker": "node ./src/scanner.js",
    "tools": "./tools/index.ts"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@11ty/eleventy-plugin-vite": "^4.0.0",
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@jetbrains/logos": "^2.2.16",
    "@rescui/icons": "^0.11.0",
    "@rescui/typography": "^0.10.0",
    "@sinclair/typebox": "^0.26.8",
    "@testing-library/dom": "^9.2.0",
    "@types/luxon": "^3.2.0",
    "@types/markdown-it": "^12.2.3",
    "@types/vhtml": "^2.2.4",
    "@types/video.js": "^7.3.50",
    "@types/yargs": "^17.0.24",
    "@vitest/coverage-c8": "^0.30.1",
    "broken-link-checker": "^0.7.8",
    "bulma": "^0.9.4",
    "chalk": "^5.3.0",
    "command-line-args": "^5.2.1",
    "consistent-hash": "^1.1.1",
    "copyfiles": "^2.4.1",
    "esbuild-register": "^3.3.2",
    "freezeframe": "^5.0.2",
    "happy-dom": "^10.2.0",
    "html-entities": "^2.3.3",
    "lunr": "^2.3.9",
    "markdown-it-prism": "^2.3.0",
    "node-html-parser": "^6.1.5",
    "patch-package": "^6.5.1",
    "plyr": "^3.7.8",
    "prettier": "^2.8.7",
    "prism-themes": "^1.9.0",
    "rimraf": "^5.0.0",
    "sass": "^1.63.6",
    "sharp": "0.32.0",
    "svgo": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "upath": "^2.0.1",
    "vhtml": "^2.2.0",
    "vite": "4.3.9",
    "vite-plugin-image-optimizer": "^1.1.2",
    "vitest": "^0.30.1",
    "yargs": "^17.7.2"
  },
  "volta": {
    "node": "19.6.0"
  }
}
