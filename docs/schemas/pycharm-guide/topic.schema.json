{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://www.jetbrains.com/guide/topic.schema.json",
  "title": "JetBrains Guide Topic",
  "properties": {
    "title": {
      "description": "Title of this resource",
      "type": "string"
    },
    "label": {
      "description": "Label of this resource",
      "type": "string"
    },
    "subtitle": {
      "description": "Subtitle of this resource",
      "type": "string"
    },
    "accent": {
      "type": "string"
    },
    "icon": {
      "type": "string"
    }
  },
  "type": "object",
  "required": [
    "title",
    "accent",
    "icon"
  ]
}